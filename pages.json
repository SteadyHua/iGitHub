{
	// 此为需要加入的内容，如果不是把uView放在根目录，请根据情况修改路径
	"easycom": {
		"^u-(.*)": "@/uview/components/u-$1/u-$1.vue"
	},
	"pages": [{
		"path": "pages/index/index",
		"style": {
			"navigationStyle": "custom",
			"navigationBarTextStyle": "white"
		}
	}, {
		"path": "pages/login/login",
		"style": {
			"titleNView": false
		}
	}, {
		"path": "pages/login/auth",
		"style": {
			"navigationBarTitleText": "安全登录"
		}
	}, {
		"path": "pages/login/auth-h5-callback",
		"style": {
			"navigationBarTitleText": "安全登录中..."
		}
	}, {
		"path": "pages/news/news",
		"style": {
			"navigationBarTitleText": "通知"
		}
	}, {
		"path": "pages/star/star",
		"style": {
			"navigationBarTitleText": "星"
		}
	}, {
		"path": "pages/search/search",
		"style": {
			"navigationStyle": "custom",
			"navigationBarTextStyle": "white"
		}
	}, {
		"path": "pages/user/user",
		"style": {
			"navigationBarTitleText": "我的"
		}
	}, {
		"path": "pages/user/base-info",
		"style": {
			"navigationBarTitleText": "基本信息"
		}
	}, {
		"path": "pages/repos/detail",
		"style": {
			"navigationBarTitleText": "仓库"
		}
	}, {
		"path": "pages/repos/code/code",
		"style": {
			"navigationBarTitleText": "代码"
		}
	}, {
		"path": "pages/repos/code/text",
		"style": {
			"navigationBarTitleText": "代码"
		}
	}    ,{
            "path" : "pages/user/setting",
            "style" : {}
        }
    ],
	"tabBar": {
		"color": "#8699a7",
		"selectedColor": "#f24713",
		"backgroundColor": "#ffffff",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/img/tabbar/trending.png",
			"selectedIconPath": "static/img/tabbar/trendingHL.png"
		}, {
			"pagePath": "pages/news/news",
			"iconPath": "static/img/tabbar/news.png",
			"selectedIconPath": "static/img/tabbar/newsHL.png"
		}, {
			"pagePath": "pages/star/star",
			"iconPath": "static/img/tabbar/star.png",
			"selectedIconPath": "static/img/tabbar/starHL.png"
		}, {
			"pagePath": "pages/search/search",
			"iconPath": "static/img/tabbar/search.png",
			"selectedIconPath": "static/img/tabbar/searchHL.png"
		}, {
			"pagePath": "pages/user/user",
			"iconPath": "static/img/tabbar/user.png",
			"selectedIconPath": "static/img/tabbar/userHL.png"
		}]
	},
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#f24713",
		"backgroundColor": "#ffffff"
	}
}
