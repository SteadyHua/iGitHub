{
	// 此为需要加入的内容，如果不是把uView放在根目录，请根据情况修改路径
	"easycom": {
		"^u-(.*)": "@/uview/components/u-$1/u-$1.vue"
	},
	"pages": [{
		"path": "pages/login/login",
		"style": {
			"titleNView": false
		}
	}, {
		"path": "pages/index/index",
		"style": {
			"navigationBarTitleText": "趋势"
			// "titleNView": false
		}
	}, {
		"path": "pages/login/auth",
		"style": {
			"navigationBarTitleText": "安全登录"
		}
	}, {
		"path": "pages/news/news",
		"style": {
			"navigationBarTitleText": "通知"
		}
	}, {
		"path": "pages/star/star",
		"style": {
			"navigationBarTitleText": "星"
		}
	}, {
		"path": "pages/search/search",
		"style": {
			"navigationBarTitleText": "搜索"
		}
	}, {
		"path": "pages/github/github",
		"style": {
			"navigationBarTitleText": "我的"
		}
	}],
	"tabBar": {
		"color": "#8699a7",
		"selectedColor": "#0068d7",
		"backgroundColor": "#ffffff",
		"list": [{
			"pagePath": "pages/index/index",
			"iconPath": "static/img/tabbar/trending.png",
			"selectedIconPath": "static/img/tabbar/trendingHL.png"
		}, {
			"pagePath": "pages/news/news",
			"iconPath": "static/img/tabbar/news.png",
			"selectedIconPath": "static/img/tabbar/newsHL.png"
		}, {
			"pagePath": "pages/star/star",
			"iconPath": "static/img/tabbar/star.png",
			"selectedIconPath": "static/img/tabbar/starHL.png"
		}, {
			"pagePath": "pages/search/search",
			"iconPath": "static/img/tabbar/search.png",
			"selectedIconPath": "static/img/tabbar/searchHL.png"
		}, {
			"pagePath": "pages/github/github",
			"iconPath": "static/img/tabbar/github.png",
			"selectedIconPath": "static/img/tabbar/githubHL.png"
		}]
	},
	"globalStyle": {
		"navigationBarTextStyle": "white",
		"navigationBarTitleText": "uni-app",
		"navigationBarBackgroundColor": "#0068d7",
		"backgroundColor": "#ffffff"
	}
}
